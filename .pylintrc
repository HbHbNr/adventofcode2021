[BASIC]
argument-naming-style=camelCase
attr-naming-style=camelCase
function-naming-style=camelCase
method-naming-style=camelCase
module-naming-style=snake_case
variable-naming-style=camelCase
good-names=_,i,j,x,y,z,x1,x2,y1,y2

[FORMAT]
expected-line-ending-format=LF
max-line-length=127

[MESSAGES CONTROL]
# do not report missing docstring
#disable=C0114,C0115,C0116,invalid-name,duplicate-code,import-outside-toplevel
disable=C0114,C0115,C0116,duplicate-code,import-outside-toplevel

[REPORTS]
score=no
